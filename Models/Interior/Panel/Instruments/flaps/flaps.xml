<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>flaps.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>index</object-name>
    
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>index</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>    
  
  <animation>
    <type>translate</type>
    <object-name>index</object-name>
    <property>surface-positions/flaps-pos-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.00 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.08 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
</PropertyList>
